						$('.modal_bg').show();
					}
					var modal_close = function() {
						$('.modal_bg').fadeOut();
						$('.modal_suc').hide();
					}

					//关闭
					$(".modal_bg").tap(function() {
						modal_close();
						$(".share").hide()
